# .env.example
# Copie este arquivo para .env e preencha os valores

# --- Configurações de Banco de Dados e Admin ---
# Usado em: lib/view/dev_tools_view.dart
# Senha para acessar o painel de ferramentas de desenvolvedor (DevTools)
DB_ADMIN_PASSWORD=seu_password_devtools_aqui #123456!@#$

# Usado em: lib/view/login_view.dart
# Email do administrador para redirecionamento específico no login
ADMIN_EMAIL=sua_email_admin_aqui #exemplo@email.com

# Usado em: lib/controller/firestore_service.dart
# Telefone padrão do administrador (WhatsApp) caso não esteja no Firestore
WHATSAPP_ADMIN=seu_numero_whatsapp_aqui #5511999999999

# --- Firebase Cloud Messaging (Push Notifications) ---
# Usado em: lib/controller/firestore_service.dart
# Chave do servidor (Legacy) para envio de notificações via HTTP v1/Legacy
FCM_SERVER_KEY=sua_server_key_aqui

# Usado em: main.dart
# Chave VAPID para identificação do servidor de push na Web
VAPID_KEY=sua_vapid_key_aqui

# --- Integrações Externas ---
# Usado em: lib/view/dev_tools_view.dart
# Chave de API do JSONBin.io para exportação de dados para a Web
X_ACCESS_API_KEY=sua_api_key_jsonbin

# Usado em: main.dart
# Chave do Google reCAPTCHA para Firebase App Check na Web
RECAPTCHA_SITE_KEY=sua_recaptcha_site_key

# --- Configurações de Teste / Mocks ---
# Usado em: test/dev_tools_view_test.dart
# Chaves utilizadas para mockar a inicialização do Firebase em testes
FIREBASE_API_KEY=sua_api_key_firebase
FIREBASE_APP_ID=seu_app_id_firebase
FIREBASE_MESSAGING_SENDER_ID=seu_messaging_sender_id_firebase
FIREBASE_PROJECT_ID=seu_project_id_firebase


# Configurações do Firebase Web
WEB_API_KEY=SUA_API_KEY_WEB_AQUI
WEB_APP_ID=SEU_APP_ID_WEB_AQUI

# Configurações do Firebase Android
ANDROID_API_KEY=SUA_API_KEY_ANDROID_AQUI
ANDROID_APP_ID=SEU_APP_ID_ANDROID_AQUI

# Configurações do Firebase iOS
IOS_API_KEY=SUA_API_KEY_IOS_AQUI
IOS_APP_ID=SEU_APP_ID_IOS_AQUI
IOS_CLIENT_ID=SEU_IOS_CLIENT_ID_AQUI

# Configurações Gerais
MESSAGING_SENDER_ID=SEU_SENDER_ID_AQUI
PROJECT_ID=SEU_PROJECT_ID_AQUI
STORAGE_BUCKET=SEU_PROJECT_ID.firebasestorage.app
IOS_BUNDLE_ID=com.example.app

# Configurações de ambiente
ENV=development

#Endereço do backend (se aplicável)
BACKEND_URL=http://localhost:3000
